/**app.wxss**/
@import 'weui.wxss';

page {
  /* background-color: #f2f2f2; */
  background-color: #f8f8f8;
  /* background-color: black; */
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

::-webkit-scrollbar {
  display: none;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
